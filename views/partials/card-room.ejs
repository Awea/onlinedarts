<% rooms.forEach((room) => { %>
  <div class="col-4">
    <div class="card">
      <div class="card-header">
        <%= room.name %>
        <% if (room.private){ %>
          <span class="oi oi-lock-locked float-right"></span>
        <% } %>
      </div><div class="card-body"></div>
    </div>
  </div>
<% }); %>
